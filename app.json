{
  "name": "Patrick's Blog",
  "keywords": [
    "ruby",
    "rails",
    "gems",
    ""
  ],
  "website": "http://patrickemuller.com",
  "repository": "https://github.com/patrickemuller/rails-blog",
  "scripts": {
    "postdeploy": "bundle exec rails db:migrate"
  },
  "env": {
    "DATABASE_URL": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-ruby"
    }
  ]
  "image": "heroku/ruby"
}
